kol = input("Wybierz gotowe zadania(dodawanie zada≈Ñ) ")

match kol:
    case "Petla":
        print("Zadanie z petla")

    case "dekorator":
        print("Dekorator")

    case "Miasta":
        print("You can become a backend developer")

    case "Kolejne_Kolokwium":
        print("Kolejne kolokwium")
